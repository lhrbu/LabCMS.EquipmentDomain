<Window x:Class="LabCMS.FixtureDomain.Client.Wpf.Pages.FixturesPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabCMS.FixtureDomain.Client.Wpf.Pages"
        mc:Ignorable="d"
        Title="FixturesPage" Height="450" Width="800"
        xmlns:pu="clr-namespace:Panuon.UI.Silver;assembly=Panuon.UI.Silver">
    <StackPanel>
        <DataGrid Name="DG" CanUserAddRows="False"
                  CanUserDeleteRows="False"
                  CanUserReorderColumns="False"
                  AutoGenerateColumns="False"
                  Margin="10"
                  pu:DataGridHelper.UnitSelectedBackground="#440021FF"
                  pu:DataGridHelper.UnitHoverBackground="#220021FF">
            <DataGridTextColumn Binding="{Binding Key}">
                
            </DataGridTextColumn>
            <DataGridTextColumn Binding="{Binding Value}" />
            
        </DataGrid>
    </StackPanel>
</Window>
